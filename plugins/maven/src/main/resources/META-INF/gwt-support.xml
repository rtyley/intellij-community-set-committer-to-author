<!DOCTYPE idea-plugin PUBLIC "Plugin/DTD" "http://plugins.intellij.net/plugin.dtd">
<idea-plugin url="http://www.jetbrains.com/idea">
  <extensions xmlns="org.jetbrains.idea.maven">
    <importer implementation="com.jetbrains.maven.gwt.GwtTotspFacetImporter" order="after web"/>
    <importer implementation="com.jetbrains.maven.gwt.GwtCodehausFacetImporter" order="after web"/>
  </extensions>

  <extensions defaultExtensionNs="com.intellij">
    <gwt.sdkType implementation="com.jetbrains.maven.gwt.GwtMavenSdkType"/>
  </extensions>
</idea-plugin>
